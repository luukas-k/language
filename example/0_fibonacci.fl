object Person {
	age: i64
}

fn fib(n: i64) -> i64 {
	if(0 == n){
		0;
	}
	else {
		if(1 == n){
			1;
		}
		else{
			this(n - 1) + this(n - 2);
		}
	}
}

fn main() -> i64 {
	let a: i64 = 6;

	print(a);
	print("fib(", 5, ") = ", fib(5));
	print("fib(", 10, ") = ", fib(10));
	print("fib(", 15, ") = ", fib(15));
	print("fib(", 20, ") = ", fib(20));
}
