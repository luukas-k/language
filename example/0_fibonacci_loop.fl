fn fib(n: i64) -> i64 {
	let i = 0;
	let p0 = 0;
	let p1 = 1;
	while(i < n){
		let p = p0 + p1;
		p0 = p1;
		p1 = p;
		i = i + 1;
	}
	p0;
}

fn main() -> i64 {
	let a: i64 = 6;

	println(a);
	println("fib(", 5, ") = ", fib(5));
	println("fib(", 10, ") = ", fib(10));
	println("fib(", 15, ") = ", fib(15));
	println("fib(", 20, ") = ", fib(20));
}
